{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@src": "./src",
          "@assets": "./assets",
          "@components": "./src/components",
          "@configs": "./src/configs",
          "@hooks": "./src/hooks",
          "@libs": "./src/libs",
          "@modules": "./src/modules",
          "@pages": "./src/pages",
          "@queries": "./src/queries",
          "@utils": "./src/utils"
        }
      }
    ]
  ]
}
